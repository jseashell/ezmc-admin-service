{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "bin/",
    "sourceMap": true,
    "target": "es2015",
    "paths": {
      "@cache": ["src/cache"],
      "@commands": ["src/commands"],
      "@utils": ["src/utils"]
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
