{
  "name": "ezmc-cli",
  "version": "0.0.7",
  "description": "CLI for self-hosting a Minecraft Java server with AWS Elastic Container Service",
  "keywords": [
    "Minecraft server",
    "AWS",
    "CLI",
    "ECS"
  ],
  "author": "John Schellinger",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "link": " npm unlink ezmc && npm link && chmod +x bin/*",
    "test": "jest --coverage",
    "format": "prettier --write '**/*.{js,ts,yml,json}' --list-different"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.632.0",
    "@aws-sdk/client-ec2": "^3.632.0",
    "@aws-sdk/client-ecs": "^3.632.0",
    "@aws-sdk/client-s3": "^3.632.0",
    "@aws-sdk/client-sts": "^3.632.0",
    "commander": "^12.1.0",
    "copy-to-clipboard": "^3.3.3",
    "ini": "^4.1.3"
  },
  "devDependencies": {
    "@swc/core": "^1.7.11",
    "@types/ini": "^4.1.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "@vercel/ncc": "^0.38.1",
    "aws-sdk-client-mock": "^4.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "bin": {
    "ezmc": "./bin/index.js"
  }
}
