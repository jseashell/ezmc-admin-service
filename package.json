{
  "name": "ezmc-cli",
  "version": "0.0.3",
  "description": "CLI for self-hosting a Minecraft Java server with AWS Elastic Container Service",
  "keywords": [
    "Minecraft server",
    "AWS",
    "CLI",
    "ECS"
  ],
  "author": "John Schellinger",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf bin",
    "build": "npm run clean && ncc build src/index.ts -o bin",
    "link": " npm unlink ezmc && npm link && chmod +x bin/*",
    "format": "prettier --write '**/*.{js,ts,yml,json}' --list-different"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.624.0",
    "@aws-sdk/client-ec2": "^3.627.0",
    "@aws-sdk/client-ecs": "^3.624.0",
    "@aws-sdk/client-s3": "^3.627.0",
    "@aws-sdk/client-sts": "^3.629.0",
    "commander": "^12.1.0",
    "copy-to-clipboard": "^3.3.3",
    "dotenv": "^16.4.5",
    "ini": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.2.0",
    "@vercel/ncc": "^0.38.1",
    "prettier": "^2.8.3",
    "typescript": "^5.5.4"
  },
  "bin": {
    "ezmc": "./bin/index.js"
  }
}
